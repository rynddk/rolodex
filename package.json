{
  "name": "rolodex",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.21.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-csv": "2.0.3",
    "react-dom": "17.0.1",
    "react-paginate": "6.5.0",
    "react-scripts": "4.0.1",
    "web-vitals": "0.2.4"
  },
  "devDependencies": {
    "axe-core": "4.1.1",
    "babel-eslint": "10.1.0",
    "cypress": "6.1.0",
    "cypress-axe": "0.12.0",
    "eslint": "7.16.0",
    "eslint-plugin-react": "7.21.5",
    "stylelint": "13.8.0"
  },
  "scripts": {
    "start": "npm run lint && react-scripts start",
    "build": "npm run lint && react-scripts build",
    "test": "npm run lint && cypress open",
    "eject": "react-scripts eject",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/**/*.js  src/**/*.jsx",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
